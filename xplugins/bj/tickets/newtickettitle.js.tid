exports.name ="newtickettitle"; 
exports.run  = function(baseTitle,options) {

		var options = options || {};
		var c = 0,
			title = baseTitle;
		while($tw.wiki.getTiddler(title) ) {
			title = baseTitle + 
				(options.prefix || "-") + 
				(++c);
		}
		return title;
}
